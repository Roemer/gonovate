{
    "platform": "noop",
    "platformConfig": {
        "inplace": true
    },
    "extends": [
        "defaults"
    ],
    "managers": [
        {
            "id": "dockerfile",
            "type": "dockerfile",
            "managerConfig": {
                "filePatterns": [
                    "latest.Dockerfile"
                ]
            },
            "dependencyConfig": {
                "ignoreNonMatching": true
            }
        },
        {
            "id": "devcontainer",
            "type": "devcontainer",
            "dependencyConfig": {
                "ignoreNonMatching": true
            }
        }
    ]
}