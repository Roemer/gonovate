{
	"name": "gonovate-dev",
	"initializeCommand": {
		"user-env": "touch ${localWorkspaceFolder}/.devcontainer/.env"
	},
	"dockerFile": "Dockerfile",
	"runArgs": [
		"--env-file=./.devcontainer/.env"
	],
	"features": {
		// gonovate: { "datasource":"docker", "matchString":"preset:docker-image-in-quotes" }
		"ghcr.io/devcontainers/features/go:1.0.0": {
			// gonovate: { "datasource":"go-version", "dependencyName":"go-stable", "matchString":"preset:json-version" }
			"version": "1.21.0"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"roemer.gotaskr-vscode"
			]
		}
	}
}