{
	"name": "gonovate-dev",
	"initializeCommand": {
		"user-env": "touch ${localWorkspaceFolder}/.devcontainer/.env"
	},
	"dockerFile": "Dockerfile",
	"runArgs": [
		"--env-file=./.devcontainer/.env"
	],
	"features": {
		"ghcr.io/devcontainers/features/go:1.3.1": {
			"version": "1.24.1"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"roemer.gotaskr-vscode"
			]
		}
	}
}