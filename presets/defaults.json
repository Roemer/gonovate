{
    "rules": [
        {
            "packageSettings": {
                "maxUpdateType": "minor",
                "versioning": "(\\d+)(?:\\.(\\d+))?(?:\\.(\\d+))?"
            }
        },
        {
            "matches": {
                "datasources": [
                    "nodejs"
                ]
            },
            "packageSettings": {
                "extractVersion": "v(.*)",
                "versioning": "^(\\d+)(?:\\.(\\d+))?(?:\\.(\\d+))?$"
            }
        }
    ]
}