{
    "rules": [
        {
            "packageSettings": {
                "maxUpdateType": "minor",
                "versioning": "(\\d+)(?:\\.(\\d+))?(?:\\.(\\d+))?"
            }
        },
        {
            "matches": {
                "datasources": [
                    "nodejs"
                ]
            },
            "packageSettings": {
                "extractVersion": "v(.*)",
                "versioning": "^(\\d+)(?:\\.(\\d+))?(?:\\.(\\d+))?$"
            }
        },
        {
            "matches": {
                "datasources": [
                    "npm"
                ]
            },
            "packageSettings": {
                "versioning": "^(\\d+)\\.(\\d+)\\.(\\d+)(?:-(.+?))?(?:[-\\.](\\d+))?(?:[-\\.](\\d+))?$"
            }
        },
        {
            "matches": {
                "datasources": [
                    "go-version"
                ]
            },
            "packageSettings": {
                "extractVersion": "go(.*)",
                "versioning": "(\\d+)(?:\\.(\\d+))?(?:\\.(\\d+))?(?:([a-z]+)(\\d+)?)?$"
            }
        }
    ]
}